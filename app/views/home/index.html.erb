<h1>Welcome to Twitter</h1>
<table>
  <tr>
    <th>Message</th>
    <th>User</th>
    <th>Posted</th>
  </tr>

<% @tweets.each do |tweet| %>
  <tr>
    <td><%= tweet.message %></td>
    <td><%= tweet.user.email %></td>
    <td><%= time_ago_in_words tweet.created_at %> ago</td>    
  </tr>
<% end %>
</table>
